tr">"return window.confirm('Are you sure you want to delete this record?');=
"</span>);

        column =3D <span xmlns:fo=3D"http://www.w3.org/1999/XSL/Format" cla=
ss=3D"kwd">new</span> Column(<span xmlns:fo=3D"http://www.w3.org/1999/XSL/F=
ormat" class=3D"str">"Action"</span>);
        column.setTextAlign(<span xmlns:fo=3D"http://www.w3.org/1999/XSL/Fo=
rmat" class=3D"str">"center"</span>);
        AbstractLink[] links =3D <span xmlns:fo=3D"http://www.w3.org/1999/X=
SL/Format" class=3D"kwd">new</span> AbstractLink[] { editLink, deleteLink }=
;
        column.setDecorator(<span xmlns:fo=3D"http://www.w3.org/1999/XSL/Fo=
