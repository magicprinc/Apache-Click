        fieldAndContainer.add(<span xmlns:fo=3D"http://www.w3.org/1999/XSL/=
Format" class=3D"kwd">new</span> TextArea(<span xmlns:fo=3D"http://www.w3.o=
rg/1999/XSL/Format" class=3D"str">"myarea"</span>));

