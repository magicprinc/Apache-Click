  <span xmlns:fo=3D"http://www.w3.org/1999/XSL/Format" class=3D"tag">&lt;he=
ad&gt;</span>
    <code class=3D"varname">$headElements</code>
  <span xmlns:fo=3D"http://www.w3.org/1999/XSL/Format" class=3D"tag">&lt;/h=
ead&gt;</span>

